import pool from "../congig/database.js";

class OpamSupervisors {
  constructor(opam_supervisor_id) {
    this.opam_supervisor_id = opam_supervisor_id;
    this.lastname = null;
    this.firstname = null;
    this.middlename = null;
    this.prc_no = null;
    this.validity = null;
    this.ptr_no = null;
    this.issued_date = null;
    this.issued_at = null;
    this.tin_no = null;
    this.ctc_no = null;
  }

  // Getters
  getOpamSupervisorId() {
    return this.opam_supervisor_id;
  }

  getLastname() {
    return this.lastname;
  }

  getFirstname() {
    return this.firstname;
  }

  getMiddlename() {
    return this.middlename;
  }

  getPrcNo() {
    return this.prc_no;
  }

  getValidity() {
    return this.validity;
  }

  getPtrNo() {
    return this.ptr_no;
  }

  getIssuedDate() {
    return this.issued_date;
  }

  getIssuedAt() {
    return this.issued_at;
  }

  getTinNo() {
    return this.tin_no;
  }

  getCtcNo() {
    return this.ctc_no;
  }

  // Setters
  setOpamSupervisorId(opam_supervisor_id) {
    this.opam_supervisor_id = opam_supervisor_id;
  }

  setLastname(lastname) {
    this.lastname = lastname;
  }

  setFirstname(firstname) {
    this.firstname = firstname;
  }

  setMiddlename(middlename) {
    this.middlename = middlename;
  }

  setPrcNo(prc_no) {
    this.prc_no = prc_no;
  }

  setValidity(validity) {
    this.validity = validity;
  }

  setPtrNo(ptr_no) {
    this.ptr_no = ptr_no;
  }

  setIssuedDate(issued_date) {
    this.issued_date = issued_date;
  }

  setIssuedAt(issued_at) {
    this.issued_at = issued_at;
  }

  setTinNo(tin_no) {
    this.tin_no = tin_no;
  }

  setCtcNo(ctc_no) {
    this.ctc_no = ctc_no;
  }
}

export default OpamSupervisors;
